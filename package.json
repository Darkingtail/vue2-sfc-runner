{
  "name": "vue2-sfc-runner",
  "version": "0.1.1",
  "packageManager": "pnpm@10.0.0",
  "description": "Vue 2 SFC runtime with compilation, sandbox, and preview support",
  "author": "darkingtail",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicepkg/vue2-sfc-runner.git"
  },
  "homepage": "https://github.com/nicepkg/vue2-sfc-runner#readme",
  "bugs": {
    "url": "https://github.com/nicepkg/vue2-sfc-runner/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "vue2-sfc-compiler": "^0.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@types/node": "^22.0.0",
    "husky": "^9.0.0",
    "semantic-release": "^24.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.0"
  },
  "keywords": [
    "vue2",
    "sfc",
    "runner",
    "preview",
    "sandbox"
  ],
  "license": "MIT"
}
